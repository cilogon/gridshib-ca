<?xml version="1.0" encoding="utf-8"?> 

<jnlp spec="1.0+" codebase="$$code_base" > 
   <information> 
      <title>GridShib CA Credential Retriever (v@PACKAGE_VERSION@)</title>
      <vendor>Von Welch</vendor>
      <homepage href="http://gridshib.globus.org"/>
      <description>Download and install my Grid credentials.</description>
      <offline-allowed/> 
    </information> 
 <security>
      <all-permissions/>
  </security>
    <resources> 
        <!-- Require Sun's JRE since we use internal PKCS10 class -->
        <j2se version="1.4+" href="http://java.sun.com/products/autodl/j2se"/> 
        <jar href="@JAR_FILE@"/> 
    </resources> 
    <application-desc main-class="edu.ncsa.gridshib.gridshibca.CredentialRetriever">
		<argument>$$genCredURL</argument>
		<argument>$$shibSession</argument>
    		<argument>$$token</argument>
  	</application-desc>
</jnlp> 
